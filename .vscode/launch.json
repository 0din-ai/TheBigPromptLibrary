{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "idxtool-Rebuild TOC",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.scripts/idxtool.py",
            "args": ["--toc"],
            "console": "integratedTerminal"
        },
        {
            "name": "idxtool-Find GPT File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.scripts/idxtool.py",
            "args": ["--find-gptfile", "https://chat.openai.com/g/g-svehnI9xP-retro-adventures"],
            "console": "integratedTerminal"
        },
        {
            "name": "idxtool-Rename",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.scripts/idxtool.py",
            "args": ["--rename"],
            "console": "integratedTerminal"
        }
    ]
}
